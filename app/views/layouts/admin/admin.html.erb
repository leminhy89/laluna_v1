  <%= csrf_meta_tags %>
</head>
<body>
    
    <body class="nav-md">
    <div class="container body">
      <div class="main_container">
        <%= render 'partials/admin/menu' %>

        <%= render 'partials/admin/topnavigation' %>
        
        <!-- page content -->
        <%= yield %>
        <!-- /page content -->
        <%= render 'partials/admin/footer' %>
      </div>
    </div>
  </body>
</body> 
</html>